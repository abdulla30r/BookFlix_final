@model IEnumerable<Feedback>

@{
    ViewData["Title"] = "Index";
}

<partial name="_Notification" />

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Feedback List</h2>
        </div>
    </div>
    <br />

    <table class="table table-bordered table-striped" style="margin:auto">
        <thead>
            <tr>
                <th style="width: 5%;">
                    Id
                </th>
                <th style="width: 25%;">
                    User Name
                </th>
                <th>Feedback Message</th>
            </tr>
        </thead>

        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td>
                        @obj.id
                    </td>
                    <td>
                        @obj.UserName
                    </td>
                    <td>
                        @obj.Message
                    </td>
                    
                </tr>
            }
        </tbody>
    </table>
</div>

